# global debug option
org.eclipse.gyrex.cloud/debug=true

# debug ZooKeeper server operation
org.eclipse.gyrex.cloud/zooKeeperServer=true

# debug ZooKeeper client connection lifecycle
org.eclipse.gyrex.cloud/zooKeeperGateLifecycle=true

# debug node's cloud state
org.eclipse.gyrex.cloud/cloudState=true

# debug node metrics reported to cloud
org.eclipse.gyrex.cloud/nodeMetrics=false

# debug lock service
org.eclipse.gyrex.cloud/zooKeeperLockService=true

# debug preferences
org.eclipse.gyrex.cloud/zooKeeperPreferences=true
org.eclipse.gyrex.cloud/zooKeeperPreferencesSync=true